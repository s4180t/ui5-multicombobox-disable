<mvc:View xmlns:form="sap.ui.layout.form" xmlns:layout="sap.ui.layout"
    controllerName="ui5multicomboboxdisable.controller.View1" xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core" displayBlock="true" xmlns="sap.m"
    xmlns:c="ui5multicomboboxdisable.control">
    <Page id="page" title="{i18n>title}" class="sapUiContentPadding">
        <form:SimpleForm editable="true">
            <Label text="MultiComboBox" />
            <c:MultiComboBoxDisablable maxSelected="{/maxSelected}">
                <c:items>
                    <core:ListItem key="1" text="Item 1" />
                    <core:ListItem key="2" text="Item 2" />
                    <core:ListItem key="3" text="Item 3" />
                    <core:ListItem key="4" text="Item 4" />
                    <core:ListItem key="5" text="Item 5" />
                </c:items>
            </c:MultiComboBoxDisablable>
            <Label text="MaxSelected" />
            <StepInput value="{/maxSelected}" />
        </form:SimpleForm>
    </Page>
</mvc:View>